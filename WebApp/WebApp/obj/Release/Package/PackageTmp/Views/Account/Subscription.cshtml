@model IEnumerable<WebApp.tbl_subscriptions>

@{
    ViewBag.Title = "Subscription";
    Layout = "~/Views/HeaderFooter.cshtml";
}

<div class="container">
    <h2>Subscription</h2>

    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>Actions</th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Email)
                </td>
                <td>
                    @Html.ActionLink("Delete", "DeleteSubscription", new { id = item.id })
                </td>
            </tr>
        }

    </table>
</div>
