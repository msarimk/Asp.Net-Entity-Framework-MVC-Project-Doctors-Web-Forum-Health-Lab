@model IEnumerable<WebApp.tbl_gallery>
@{
    ViewBag.Title = "Gallery";
    Layout = "~/Views/FrontHeaderFooter.cshtml";
}
<!-- Start Gallery -->
<div id="gallery" class="gallery-box">
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-12">
                <div class="title-box">
                    <h2>Gallery</h2>
                    @*<p>Lorem ipsum dolor sit amet, consectetur adipiscing elit. </p>*@
                </div>
            </div>
        </div>

        <div class="popup-gallery row clearfix">

            @foreach(var data in Model) {

            <div class="col-md-3 col-sm-6">
                <div class="box-gallery">
                    <img src="~/GalleryImages/@data.Images" alt="">
                    <div class="box-content">
                        <h3 class="title">@data.Titles</h3>
                        <ul class="icon">
                            <li><a href="images/gallery-01.jpg"><i class="fa fa-picture-o" aria-hidden="true"></i></a></li>
                        </ul>
                    </div>
                </div>
            </div>
            }


        </div>
    </div>
</div>
<!-- End Gallery -->
