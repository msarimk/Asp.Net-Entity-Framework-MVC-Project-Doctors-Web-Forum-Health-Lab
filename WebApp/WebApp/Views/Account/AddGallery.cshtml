
@{
    ViewBag.Title = "AddGallery";
    Layout = "~/Views/HeaderFooter.cshtml";
}

<!-- Begin Page Content -->
<div class="container">
    <div class="d-sm-flex align-items-center justify-content-between mb-4">
        <h1 class="h3 mb-0 text-gray-800">Add Gallery Images</h1>
    </div>

    <form action="~/Account/AddGallery" method="post" enctype="multipart/form-data">
        <div class="form-group col-xxl-5 col-xl-5 col-lg-5 col-md-5   mx-auto">


            <label for="exampleInputEmail1">Title</label>
            <input name="title" type="text" class="form-control" aria-describedby="emailHelp" placeholder="Enter title">



            <label for="exampleInputPassword1" class="mt-3">Gallery Images</label>
            <input name="galleryimg" type="file" class="form-control">


            <button type="submit" name="submit" class="btn btn-primary mt-5">Submit</button>

        </div>
    </form>

</div>
<!-- End of Main Content -->
